    "compilerOptions": {
        "target": "ES2017",
        "module": "commonjs",
        "allowJs": true,
        "isolatedModules": false,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": ["**/*"],
    "exclude": ["node_modules", "coverage"]
}
